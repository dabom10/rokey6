mpose([transforms.ToTensor()])\n","train_ds = datasets.MNIST(root='/tmp/mnist', train=True, download=True, transform=tfm)\n","test_ds  = datasets.MNIST(root='/tmp/mnist', train=False, download=True, transform=tfm)\n","# pin_memory는 GPU 쓸때, num_worker=2는 프로세스 2개동작\n","train_loader = DataLoader(train_ds, batch_size=256, shuffle=True, num_workers=2, pin_memory=True)\n","test_loader  = DataLoader(test_ds,  batch_size=512, shuffle=False, num_workers=2, pin_memory=True)\n","\n","# MLP = multi layer perceptron 약자. = 다층 퍼셉트론\n","class MLP(nn.Module):\n","    def __init__(self):\n","        super().__init__()\n","        self.f = nn.Sequential(\n","            # 처음부터 1차원으로 만듦.\n","            nn.Flatten(),\n","            # 28x28 이미지를 512차원으로 출력하고 ReLU 은닉층 통과.\n","            nn.Linear(28*28, 512), nn.ReLU(),\n","            # 512->256차원(뉴런 수)\n","            nn.Linear(512, 256), nn.ReLU(),\n","            # 256->10개의 층으로 간다.\n","            nn.Linear(256, 10)\n","        )\n","        # He 초기화 (ReLU에 적합)\n","        for m in self.f:\n","          # if isinstance = is instance, 선형회귀 레이어(층)이 있다면\n","            if isinstance(m, nn.Linear):\n","                nn.init.kaiming_normal_(m.weight) # He 초기화\n","                nn.init.zeros_(m.bias) # bias=0(편향=0으로 초기화)\n","    def forward(self, x): return self.f(x)\n","\n","# 모델 적용\n","model = MLP().to(device)\n","# AdamW : adam(adaptive momentom) + weight decay(가중치 감쇠) 모델\n","opt = optim.AdamW(model.parameters(), lr=3e-3) # W, b 조정해서 최적화\n","# lr_scheduler 중 onecycleLR 쓸거임(처음에는 학습률 증가-빠르게, 나중에는 감소-세밀하게)\n","# steps_per_epoch : 한 에폭마다 step을 밟아: 전체 파일의 길이만큼(에폭=5)\n","sched = optim.lr_scheduler.OneCycleLR(opt, max_lr=3e-3, steps_per_epoch=len(train_loader), epochs=5)\n","crit = nn.CrossEntropyLoss()\n","\n","# Grad 히스토그램 수집용(기울기/weight 가중치 모니터링) ** 추가 기능:가점\n","grads = []\n","# autograd hook #??\n","def hook_fn(m, gi, go):\n","    if isinstance(m, nn.Linear): # 선형 회귀함수가 있으면\n","        if m.weight.grad is not None: # weight가 None이 아니라면\n","            # datach : 계산 그래프에서 분리(순간포착, 메모리절약) #??\n","            # abs().mean(): 절댓값의 평균, item():tensor>>python의 숫자numpy의 array로 변환(**)\n","            # abs().mean() 까지 하면 1차원 텐서인데 .item()을 해야 숫자로 나온다\n","            # datach().abs() : 음수->양수 변환, mean(): 차원축소(**)\n","            grads.append(m.weight.grad.detach().abs().mean().item())\n","\n","            # 차원축소 예시\n","            # shape(256,10)\n","            # mean()으로 차원축소(256*10 = 2,560) tensor(0.0034..) 이런 식으로 바뀜\n","            # 차원축소\n","            # tensor 타입에서는 numpy.array 의 mean 매서드를 쓸 수 없으니까 바꿔줬다고 생각하면 됨 # ??\n","\n","# 리스트 컴프리헨션(15:30분 정도에서... 강의 다시보기) #??\n","hooks = [m.register_full_backward_hook(hook_fn) for m in model.modules() if isinstance(m, nn.Linear)]\n","\n","def train_epoch():\n","    model.train(); tot=0; correct=0 # accuarcy 하려고\n","    for x,y in train_loader:\n","        x,y = x.to(device), y.to(device) # gpu 씀\n","        opt.zero_grad()\n","        out = model(x); loss = crit(out, y) # 예측, 손실계산\n","        loss.backward(); opt.step(); sched.step() # 손실 업데이트, sched.step() 학습률 설정하는 lr_schedular 쓰는거 강력추천(성능 업업)\n","        tot += y.size(0); correct += (out.argmax(1)==y).sum().item() # argmax:가장 큰 값\n","    return loss.item(), correct/tot\n","    # out.argmax(1) : 각 샘플의 최대 확률이 있는 클래스  #?? 샘플이랑 배치랑 달라?\n","\n","# 모델 평가\n","def eval_epoch():\n","    model.eval(); tot=0; correct=0\n","    with torch.no_grad():\n","        for x,y in test_loader:\n","            x,y = x.to(device), y.to(device)\n","            out = model(x)\n","            tot += y.size(0); correct += (out.argmax(1)==y).sum().item()\n","    return correct/tot\n","\n","hist_grad = []  # 한 번 학습할 떄(epoch) gradient 기울기(가중치) 저장\n","hist_acc  = [] # 정확도\n","for ep in range(5):\n","    loss, tr_acc = train_epoch() # train_epoch[0] = loss 값 , train_epoch[1] = accuarcy\n","    acc = eval_epoch()\n","    # grads[-len(train_loader):] : -1 = 끝값,\n","    # -n : 끝에서 n번째 값 / 마지막 epoch에 나온 기울기/가중치/gradient만 쓰겠다\n","    hist_grad.append(sum(grads[-len(train_loader):])/max(1,len(train_loader)))\n","    hist_acc.append(acc)\n","    print(f\"EP{ep+1} loss={loss:.3f} test_acc={acc:.3f}\")\n","\n","# average |grad|와 test acc가 반비례하는 추이 보이네요 / grad가 loss와 관련있기 때문.\n","plt.figure(); plt.plot(hist_grad); plt.title('Average |grad|'); plt.show()\n","plt.figure(); plt.plot(hist_acc); plt.title('Test ACC'); plt.show()\n","\n","# 한번씩 돌고 더이상 필요없으니까 제거\n","for h in hooks: h.remove()\n"]},{"cell_type":"code","source":[],"metadata":{"id":"r31QsnwgdKJp"},"execution_count":null,"outputs":[]}]}