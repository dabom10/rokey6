ure(figsize=(10, 6))\n","bars = plt.bar(strategies, accuracies, color=['#3498db', '#2ecc71', '#e74c3c'],\n","               alpha=0.8, edgecolor='black', linewidth=1.5)\n","\n","\n","# 각 막대 위에 정확도 값 표시\n","for bar in bars:\n","    height = bar.get_height()\n","    plt.text(bar.get_x() + bar.get_width()/2., height,\n","            f'{height:.2f}%',\n","            ha='center', va='bottom', fontsize=12, fontweight='bold')\n","\n","\n","# 그래프 설정\n","plt.ylabel('Test Accuracy (%)', fontsize=12, fontweight='bold')\n","plt.title('Transfer Learning Strategy Comparison', fontsize=14, fontweight='bold')\n","plt.ylim([0, max(accuracies) * 1.15])  # Y축 범위 설정\n","plt.grid(True, axis='y', alpha=0.3, linestyle='--')\n","\n","\n","# 그래프 표시\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":472},"id":"IYRMEOo0r_e1","executionInfo":{"status":"ok","timestamp":1762843960920,"user_tz":-540,"elapsed":441,"user":{"displayName":"김다봄","userId":"02209919459503367304"}},"outputId":"8d94fee0-074e-4e9e-aa4e-b15f0ff4e615"},"execution_count":25,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]}]}