=DefaultDataCollator(return_tensors=\"pt\"),\n","            compute_metrics=metrics\n","        )\n","\n","trainer.train()\n","# 테스트 데이터 평가\n","# test 데이터셋이 존재한다고 가정 (beans[\"test\"])\n","print(trainer.evaluate(beans[\"test\"]))"]},{"cell_type":"code","execution_count":null,"id":"be7d5813","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"be7d5813","executionInfo":{"status":"ok","timestamp":1763479535850,"user_tz":-540,"elapsed":44,"user":{"displayName":"주식회사한시경","userId":"01036671795204052858"}},"outputId":"7cc804d6-6f4b-45a1-c238-18caff28db9f"},"outputs":[{"output_type":"stream","name":"stdout","text":["[0] 예측: angular_leaf_spot | 정답: angular_leaf_spot\n","[1] 예측: angular_leaf_spot | 정답: angular_leaf_spot\n"]}],"source":["import torch\n","\n","for i in [0, 1]:\n","    ex = beans[\"test\"][i]\n","\n","    # 1. 입력 데이터 처리\n","    # ex[\"pixel_values\"]가 이미 텐서이므로 바로 사용하되, 복사본을 만들어 안전하게 처리\n","    # detach : gradient 연결 끊기\n","    input_tensor = ex[\"pixel_values\"].clone().detach()\n","    # 배치 차원 추가 >> gpu 이동\n","    # .unsqueeze(0) : 맨 앞에 차원 추가(배치)\n","    # [3, 224, 224] >> [1, 3, 224, 224]\n","    inputs = input_tensor.unsqueeze(0).to(model.device)\n","\n","    #\n","    # 2. 모델 예측\n","    with torch.no_grad():\n","      logits = model(inputs).logits\n","      # 예) logits = [[-2.0, 0.5, 3.5]]\n","      # argmax : 2 (세번째 가장 높음(3.5))\n","      pred = logits.argmax(-1).item() # 2, 마지막 차원에서 최대값 인덱스 선택\n","\n","    # 3. 정답 라벨 가져오기 (수정된 부분)\n","    # 'labels' 혹은 'label' 키 확인\n","    label_key = 'labels' if 'labels' in ex else 'label'\n","\n","    # 핵심 수정: .item()을 붙여서 tensor(0) -> 0 (정수)으로 변환\n","    true_label_id = ex[label_key].item()  #\n","\n","    # 4. 결과 출력\n","    print(f\"[{i}] 예측: {model.config.id2label[pred]} | 정답: {model.config.id2label[true_label_id]}\")"]},{"cell_type":"code","source":["# EOS"],"metadata":{"id":"0yjTXA4xkXFe"},"id":"0yjTXA4xkXFe","execution_count":null,"outputs":[]},{"cell_type":"code","source":[],"metadata":{"id":"XpeIL7fYk